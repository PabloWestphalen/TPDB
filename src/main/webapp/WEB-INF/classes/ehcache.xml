<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../main/config/ehcache.xsd">
	<cache name="SimplePageCachingFilter" maxElementsOnDisk="1000"
		eternal="false" overflowToDisk="true" timeToIdleSeconds="150"
		timeToLiveSeconds="300" memoryStoreEvictionPolicy="LFU" />
	<defaultCache eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="true"
		diskPersistent="false"
		diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LRU" />s
</ehcache>